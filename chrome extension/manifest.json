{ "manifest_version": 2,
  "name": "SummaryDo",
  "description": " it'll summerize the product information into voice or just text",
  "version": "1.0",
  "author": "SummaryDo Team",

  "content_scripts" : [
    {
      "matches" :["<all_urls>"],
      "js":["content.js"]
   }
   ],
     "background":{"scripts":["background.js"] },
     "browser_action": {
       "default_icon": "icon.png",
       "default_popup": "popup.html",
       "default_title" : "To summerize products info"

     }
     }
